<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate='1'>

        <record id="ir_seq_hr_loan" model="ir.sequence">
            <field name="name">Loan Request</field>
            <field name="code">hr.loan.seq</field>
            <field name="prefix">LO/</field>
            <field name="padding">4</field>
            <field name="number_increment">1</field>
            <field name="number_next_actual">1</field>
            <field name="implementation">standard</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="ir_seq_hr_advance" model="ir.sequence">
            <field name="name">Salary Advance Request</field>
            <field name="code">hr.salary.advance.seq</field>
            <field name="prefix">SAR</field>
            <field name="padding">4</field>
            <field name="number_increment">1</field>
            <field name="number_next_actual">1</field>
            <field name="implementation">standard</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="ir_seq_other_discounts" model="ir.sequence">
			<field name="name">Other Discounts</field>
			<field name="code">hr.other.discounts.seq</field>
			<field name="prefix">DES/</field>
			<field name="padding">4</field>
			<field name="number_increment">1</field>
			<field name="number_next_actual">1</field>
			<field name="implementation">standard</field>
            <field name="company_id" eval="False"/>
		</record>

    </data>
</odoo>